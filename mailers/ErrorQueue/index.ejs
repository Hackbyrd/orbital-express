Error: <strong><%= locals.error ? error.name : 'Name' %></strong><br/>
Message: <strong><%= locals.error ? error.message : 'Message' %></strong><br/><br/>
Stack: <strong><%= locals.error ? error.stack : 'Stack' %></strong><br/><br/>

Time: <strong><%= locals.time ? time : 'Tuesday, September 12, 2017 12:07 PM' %></strong><br/>
Queue: <strong><%= locals.queue ? queue.name : 'AdminQueue' %></strong><br/>

<% locals.job = locals.job || { name: 'ADMINTASK', id: '76567', data: { key1: 'value1', key2: 'value2' } } %>

<% if (locals.job) { %>
  Job: <strong><%= job.name %></strong><br/>
  ID: <strong><%= job.id %></strong><br/>

  <% var tempArgs = job.data %>
  <% var keys = Object.keys(tempArgs); %><br/>

  <% for (var i = 0; i < keys.length; i++) { %>
    <%= keys[i] %>: <strong><%= tempArgs[keys[i]] %></strong><br/>
  <% } %>
<% } %>
